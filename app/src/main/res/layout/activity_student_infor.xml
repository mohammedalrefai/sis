<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"

    android:layout_height="match_parent"

    android:background="@drawable/sis"

    tools:context=".student_infor">



    <ImageView

        android:id="@+id/imageView23"

        android:layout_width="347dp"

        android:layout_height="426dp"

        android:layout_marginStart="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginBottom="8dp"

        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.952"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.0"

        app:srcCompat="@drawable/student_info" />



    <Button

        android:id="@+id/scholarship_bt"

        android:layout_width="219dp"

        android:layout_height="49dp"

        android:layout_marginStart="8dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginBottom="104dp"

        android:background="@drawable/scholarship_bt"

        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.566"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.917" />



    <Button

        android:id="@+id/log_out_bt"

        android:layout_width="150dp"

        android:layout_height="60dp"

        android:layout_marginStart="16dp"

        android:layout_marginTop="250dp"

        android:layout_marginEnd="8dp"

        android:layout_marginBottom="16dp"

        android:background="@drawable/log_out"

        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toStartOf="@+id/back_bt"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="1.0" />



    <Button

        android:id="@+id/back_bt"

        android:layout_width="150dp"

        android:layout_height="60dp"

        android:layout_marginStart="8dp"

        android:layout_marginTop="250dp"

        android:layout_marginEnd="16dp"

        android:layout_marginBottom="16dp"

        android:background="@drawable/back"

        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toEndOf="@+id/log_out_bt"

        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="1.0" />


    <LinearLayout

        android:id="@+id/linearLayout"

        android:layout_width="198dp"

        android:layout_height="395dp"

        android:layout_marginStart="8dp"

        android:layout_marginTop="8dp"

        android:layout_marginEnd="8dp"

        android:layout_marginBottom="8dp"

        android:orientation="vertical"

        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.31"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.100000024">


        <TextView

            android:id="@+id/st_name"

            android:layout_width="match_parent"

            android:layout_height="39dp"

            android:paddingTop="10dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/colleage"

            android:layout_width="match_parent"

            android:layout_height="37dp"

            android:paddingTop="8dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/department"

            android:layout_width="match_parent"

            android:layout_height="33dp"

            android:paddingTop="8dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/study_filed"

            android:layout_width="match_parent"

            android:layout_height="43dp"

            android:paddingTop="10dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/academic_status"

            android:layout_width="match_parent"

            android:layout_height="39dp"

            android:paddingTop="8dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/gender"

            android:layout_width="match_parent"

            android:layout_height="37dp"

            android:paddingTop="10dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/expected_graduate"

            android:layout_width="match_parent"

            android:layout_height="43dp"

            android:paddingTop="8dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/regstration_date"

            android:layout_width="match_parent"

            android:layout_height="35dp"

            android:paddingTop="10dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/regstration_date_1"

            android:layout_width="match_parent"

            android:layout_height="50dp"

            android:paddingTop="10dp"
            android:text="TextView"
            android:textAlignment="center" />


        <TextView

            android:id="@+id/regstration_date_2"

            android:layout_width="match_parent"

            android:layout_height="39dp"

            android:paddingTop="10dp"
            android:text="TextView"
            android:textAlignment="center" />

    </LinearLayout>



</android.support.constraint.ConstraintLayout>