<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".study_plane"
    android:background="@drawable/sis">

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059"
        app:srcCompat="@drawable/study_plane2" />

    <LinearLayout
        android:layout_width="337dp"
        android:layout_height="283dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView34"
        app:layout_constraintVertical_bias="0.37">

        <LinearLayout
            android:layout_width="74dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/st_hours_1"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/st_hours_2"
                android:layout_width="match_parent"
                android:layout_height="51dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/st_hours_3"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/st_hours_4"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/st_hours_5"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/st_hours_6"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="71dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/max_hours_1"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/max_hours_2"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/max_hours_3"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/max_hours_4"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/max_hours_5"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/max_hours_6"
                android:layout_width="match_parent"
                android:layout_height="52dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="72dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/hours_number_1"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/hours_number_2"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/hours_number_3"
                android:layout_width="match_parent"
                android:layout_height="51dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/hours_number_4"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/hours_number_5"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/hours_number_6"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:paddingTop="15dp"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tx_view_1"
                android:layout_width="match_parent"
                android:layout_height="42dp" />

            <TextView
                android:id="@+id/tx_view_2"
                android:layout_width="match_parent"
                android:layout_height="51dp" />

            <TextView
                android:id="@+id/tx_view_3"
                android:layout_width="match_parent"
                android:layout_height="50dp" />

            <TextView
                android:id="@+id/tx_view_4"
                android:layout_width="match_parent"
                android:layout_height="41dp" />

            <TextView
                android:id="@+id/tx_view_5"
                android:layout_width="match_parent"
                android:layout_height="51dp" />

            <TextView
                android:id="@+id/tx_view_6"
                android:layout_width="match_parent"
                android:layout_height="49dp" />
        </LinearLayout>
    </LinearLayout>

    <Button
        android:id="@+id/log_out_bt"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/log_out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/back_bt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.96" />

    <Button
        android:id="@+id/back_bt"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.631"
        app:layout_constraintStart_toEndOf="@+id/log_out_bt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.96" />

</android.support.constraint.ConstraintLayout>